{
  "name": "nuxt3-admin",
  "private": true,
  "description": "Admin-panel created on nuxt3 with bootstrap",
  "author": "Jardaxion",
  "license": "GNU AGPLv3",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jardaxion/nuxt3-admin.git"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "help": "npx ts-node --esm dev/commands/helps/help.ts",
    "add:help": "npx ts-node --esm dev/commands/helps/helpAdd.ts",
    "remove:help": "npx ts-node --esm dev/commands/helps/helpAdd.ts",
    "add:model": "npx ts-node --esm dev/commands/add/addModel.ts",
    "remove:model": "npx ts-node --esm dev/commands/remove/removeModel.ts",
    "add:controller": "npx ts-node --esm dev/commands/add/addController.ts",
    "remove:controller": "npx ts-node --esm dev/commands/remove/removeController.ts",
    "add:command": "npx ts-node --esm dev/commands/add/addCommand.ts",
    "remove:command": "npx ts-node --esm dev/commands/remove/removeCommand.ts"
  },
  "overrides": {
    "vue": "latest"
  },
  "devDependencies": {
    "@types/node": "^18",
    "nuxt": "^3.5.2",
    "sweetalert2": "^11.7.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vue-sweetalert2": "^5.0.5"
  },
  "dependencies": {
    "@kyvg/vue3-notification": "^2.9.1",
    "@pinia/nuxt": "^0.4.11",
    "@popperjs/core": "^2.11.8",
    "@sweetalert2/theme-bootstrap-4": "^5.0.15",
    "@sweetalert2/theme-dark": "^5.0.15",
    "bootstrap": "^5.3.0",
    "colors": "^1.4.0",
    "commander": "^10.0.1",
    "dotenv": "^16.1.4",
    "mysql2": "^3.3.5",
    "pinia": "^2.1.3",
    "sass": "^1.62.1"
  },
  "type": "module"
}
